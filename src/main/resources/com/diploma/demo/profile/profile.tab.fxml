<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.diploma.demo.view.auth.profileController">

    <Label fx:id ="role_label"></Label>
    <Label>User Name</Label>
    <TextField fx:id ="tfLogin"></TextField>
    <Label>password</Label>
    <PasswordField fx:id="pfPassword"></PasswordField>
    <Button fx:id="btnLogin" onAction="#login">LOGIN</Button>
    <Button fx:id="btnLogout" onAction="#logout">LOGOUT</Button>


</VBox>